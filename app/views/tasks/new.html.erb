        <%= form.text_field :title,rows:5, class: 'form-control'%>
      </div>
      <div class="task_title">
        <%= form.label :内容 %>
        <br>
        <%= form.text_field :content, class: 'form-control'%>
      </div>
      <div class="task_title">
      <%= form.label :締切期限 %>
